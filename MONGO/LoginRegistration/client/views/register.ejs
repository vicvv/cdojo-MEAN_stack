<%- include('header') -%>

<h1>Register</h1>

<form method="POST" action="/users/register">
    <div id="form-group"><label>Name:</label><input class="form-control" name="name" type="text"></div>
    <div id="form-group"><label>Email:</label><input class="form-control" name="email" type="text"></div>
    <div id="form-group"><label>Username:</label><input class="form-control" name="username" type="text"></div>
    <div id="form-group"><label>Password:</label><input class="form-control" name="password" type="password"></div>
    <div id="form-group">
        <div id="form-group"><label>Birthday:</label><input class="form-control" name="birthday" type="date"></div>
    </div>
    <br><input class="btn btn-primary" type="submit" value="Submit">
</form><br>
<hr>
        
<%- include('footer') -%>